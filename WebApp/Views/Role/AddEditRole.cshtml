@model Core.Entities.Abstract.sp_GetRole


<div class="row">
    <div class="col-xl-12">
        <div style="margin:5px 5px  5px 5px">
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Id</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtId" type="number" disabled="disabled" value="@if (Model != null) @Model.Id">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container" hidden="hidden">
                <label class="col-xl-3 col-lg-3 col-form-label">Role Type Id</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtRoleTypeId" type="number" disabled="disabled" value="@if (Model != null) @Model.RoleTypeId">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container" hidden="hidden">
                <label class="col-xl-3 col-lg-3 col-form-label">Role Form Definition Id</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtRoleFormDefinitionId" type="number" disabled="disabled" value="@if (Model != null) @Model.RoleFormDefinitionId">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Açıklama</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtDescription" type="text" disabled="disabled" placeholder="Açıklama" value="@if (Model != null) @Model.Description">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Form Adı</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtFormName" type="text" disabled="disabled" placeholder="Form Adı" value="@if (Model != null) @Model.FormName">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Alt Form Adı</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtFormSubName" type="text" disabled="disabled" placeholder="Alt Form Adı" value="@if (Model != null) @Model.FormSubName">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkShow" @Html.Raw((Model != null) ? (((Model.Show) ? "checked" : "")) : "checked")>
                        <span></span> Göster
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkInsert" @Html.Raw((Model != null) ? (((Model.Insert) ? "checked" : "")) : "checked")>
                        <span></span> Ekle
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkUpdate" @Html.Raw((Model != null) ? (((Model.Update) ? "checked" : "")) : "checked")>
                        <span></span> Düzenle
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkDelete" @Html.Raw((Model != null) ? (((Model.Delete) ? "checked" : "")) : "checked")>
                        <span></span> Sil
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkPrint" @Html.Raw((Model != null) ? (((Model.Print) ? "checked" : "")) : "checked")>
                        <span></span> Yazdır
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkExport" @Html.Raw((Model != null) ? (((Model.Export) ? "checked" : "")) : "checked")>
                        <span></span> Dışarı Aktar
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkSpecialRole1" @Html.Raw((Model != null) ? (((Model.SpecialRole1) ? "checked" : "")) : "checked")>
                        <span></span> @if (Model.SpecialRole1Description == null || string.IsNullOrEmpty(Model.SpecialRole1Description)) @string.Format("Yetki 1") else @string.Format("{0}", Model.SpecialRole1Description)
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkSpecialRole2" @Html.Raw((Model != null) ? (((Model.SpecialRole2) ? "checked" : "")) : "checked")>
                        <span></span> @if (Model.SpecialRole2Description == null || string.IsNullOrEmpty(Model.SpecialRole2Description))@string.Format("Yetki 2") else @string.Format("{0}", Model.SpecialRole2Description)
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkSpecialRole3" @Html.Raw((Model != null) ? (((Model.SpecialRole3) ? "checked" : "")) : "checked")>
                        <span></span> @if (Model.SpecialRole3Description == null || string.IsNullOrEmpty(Model.SpecialRole3Description))@string.Format("Yetki 3") else @string.Format("{0}", Model.SpecialRole3Description)
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkSpecialRole4" @Html.Raw((Model != null) ? (((Model.SpecialRole4) ? "checked" : "")) : "checked")>
                        <span></span> @if (Model.SpecialRole4Description == null || string.IsNullOrEmpty(Model.SpecialRole4Description))@string.Format("Yetki 4") else @string.Format("{0}", Model.SpecialRole4Description)
                    </label>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label"></label>
                <div class="col-lg-9 col-xl-9">
                    <label class="checkbox">
                        <input type="checkbox" id="chkSpecialRole5" @Html.Raw((Model != null) ? (((Model.SpecialRole5) ? "checked" : "")) : "checked")>
                        <span></span> @if (Model.SpecialRole5Description == null || string.IsNullOrEmpty(Model.SpecialRole5Description))@string.Format("Yetki 5") else @string.Format("{0}", Model.SpecialRole5Description)
                    </label>
                </div>
            </div>


        </div>
    </div>
</div>
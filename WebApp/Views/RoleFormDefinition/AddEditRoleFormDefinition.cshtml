@model Core.Entities.Concrete.RoleFormDefinition


<div class="row">
    <div class="col-xl-12">
        <div style="margin:5px 5px  5px 5px">
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Id</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtId" type="number" disabled="disabled" value="@if (Model != null) @Model.Id">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Açıklama</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtDescription" type="text" placeholder="Açıklama" value="@if (Model != null) @Model.Description">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Form Adı</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtFormName" type="text" placeholder="Form Adı" value="@if (Model != null) @Model.FormName">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Alt Form Adı</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtFormSubName" type="text" placeholder="Alt Form Adı" value="@if (Model != null) @Model.FormSubName">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Yetki 1 Açıklama</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtSpecialRole1Description" type="text" placeholder="Yetki 1 Açıklama" value="@if (Model != null) @Model.SpecialRole1Description">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Yetki 2 Açıklama</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtSpecialRole2Description" type="text" placeholder="Yetki 2 Açıklama" value="@if (Model != null) @Model.SpecialRole2Description">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Yetki 3 Açıklama</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtSpecialRole3Description" type="text" placeholder="Yetki 3 Açıklama" value="@if (Model != null) @Model.SpecialRole3Description">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Yetki 4 Açıklama</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtSpecialRole4Description" type="text" placeholder="Yetki 4 Açıklama" value="@if (Model != null) @Model.SpecialRole4Description">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
            <div class="form-group row fv-plugins-icon-container">
                <label class="col-xl-3 col-lg-3 col-form-label">Yetki 5 Açıklama</label>
                <div class="col-lg-9 col-xl-9">
                    <input class="form-control" id="txtSpecialRole5Description" type="text" placeholder="Yetki 5 Açıklama" value="@if (Model != null) @Model.SpecialRole5Description">
                    <div class="fv-plugins-message-container"></div>
                </div>
            </div>
        </div>
    </div>
</div>